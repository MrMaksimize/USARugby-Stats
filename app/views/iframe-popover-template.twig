{% spaceless %}
<div class='popover' id='{{ entity }}-{{ id }}'>
    <div class='arrow'></div>
    <div class='popover-inner'>
        <h3 class='popover-title'>
        </h3>
        <div class='popover-content'>
            <iframe class='{{ entity }}-popover-iframe' id='{{ entity }}-{{ id }}' seamless scrolling='no' src='{{ iframe_src }}' onLoad='iframeLoaded(this)'></iframe>
        </div>
    </div>
</div>
{% endspaceless %}